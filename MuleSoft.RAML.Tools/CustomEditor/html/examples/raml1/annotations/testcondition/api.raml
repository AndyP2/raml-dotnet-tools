#%RAML 1.0
title: Post Condition Example
annotationTypes:
  PostCondition:
    parameters:
      cond: expression
types:
  expression: string
  User:
    type: object
    properties:
      id: number
      name: string
/users:
  get:
    responses:
      200:
        body:
          application/json:
            schema: User[]
            (PostCondition):
                cond: $this.eachOfThem(id!=null)
